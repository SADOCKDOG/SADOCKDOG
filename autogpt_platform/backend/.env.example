# AutoGPT Platform backend .env.example
# Copia como .env y sustituye los valores marcados.

# ===== DB =====
DB_USER=postgres
DB_PASS=CHANGE_ME_POSTGRES_PASSWORD_LONG
DB_NAME=postgres
DB_PORT=5432
DB_HOST=localhost
DB_CONNECTION_LIMIT=12
DB_CONNECT_TIMEOUT=60
DB_POOL_TIMEOUT=300
DB_SCHEMA=platform
DATABASE_URL="postgresql://${DB_USER}:${DB_PASS}@${DB_HOST}:${DB_PORT}/${DB_NAME}?schema=${DB_SCHEMA}&connect_timeout=${DB_CONNECT_TIMEOUT}"
DIRECT_URL="postgresql://${DB_USER}:${DB_PASS}@${DB_HOST}:${DB_PORT}/${DB_NAME}?schema=${DB_SCHEMA}&connect_timeout=${DB_CONNECT_TIMEOUT}"
PRISMA_SCHEMA="postgres/schema.prisma"

# ===== Redis / RabbitMQ =====
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=
RABBITMQ_DEFAULT_USER=rabbitmq_user_default
RABBITMQ_DEFAULT_PASS=CHANGE_ME_RABBITMQ_PASS

# ===== Supabase Auth =====
SUPABASE_URL=http://127.0.0.1:8000
SUPABASE_SERVICE_ROLE_KEY=PLACEHOLDER_SERVICE_ROLE_KEY
JWT_VERIFY_KEY=CHANGE_ME_JWT_SECRET_AT_LEAST_32_CHARS

# ===== Security Keys =====
# Genera con Python: from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())
ENCRYPTION_KEY=CHANGE_ME_FERNET_KEY
UNSUBSCRIBE_SECRET_KEY=CHANGE_ME_UNSUBSCRIBE_SECRET

# ===== Platform URLs =====
PLATFORM_BASE_URL=http://127.0.0.1:8000
FRONTEND_BASE_URL=http://127.0.0.1:3000

# ===== OAuth (servidor) =====
# Callback esperado del backend para integraciones: http://127.0.0.1:3000/auth/integrations/oauth_callback
GITHUB_CLIENT_ID=CHANGE_ME_GITHUB_CLIENT_ID
GITHUB_CLIENT_SECRET=CHANGE_ME_GITHUB_CLIENT_SECRET

# ===== Optional API keys =====
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GROQ_API_KEY=

# NOTA: No pongas secretos reales en control de versiones.
