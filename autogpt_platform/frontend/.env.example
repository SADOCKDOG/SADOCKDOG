# AutoGPT Platform frontend .env.example
# Copia a .env y rellena únicamente variables NEXT_PUBLIC_* y URLs.

# Visible en el navegador (no pongas secretos aquí)
NEXT_PUBLIC_SITE_URL=http://127.0.0.1:3000
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:8000
NEXT_PUBLIC_AGPT_SERVER_URL=http://127.0.0.1:8006/api
NEXT_PUBLIC_AGPT_WS_SERVER_URL=ws://127.0.0.1:8001/ws
NEXT_PUBLIC_FRONTEND_BASE_URL=http://127.0.0.1:3000
NEXT_PUBLIC_BEHAVE_AS=LOCAL

# Callback de OAuth en el frontend
AUTH_CALLBACK_URL=http://127.0.0.1:3000/auth/callback

# NOTA: No incluyas claves secretas en el frontend.
